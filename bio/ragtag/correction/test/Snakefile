rule correction:
    input:
        query="fasta/{query}.fasta",
        reference="fasta/{reference}.fasta",
    output:
        "{query}_corrected_{reference}/ragtag.correct.fasta",
        "{query}_corrected_{reference}/ragtag.correct.agp",
    params:
        extra="",
    threads: 16
    log:
        "logs/ragtag/{query}_{reference}.log",
    wrapper:
        "master/bio/ragtag/correction"
