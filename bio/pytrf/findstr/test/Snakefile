# SAMPLE RULE: Custom parameters via extra
rule pytrf_findstr:
    input:
        "demo_data/{sample}.fasta",
    output:
        "results/{sample}.csv",
    params:
        extra="-r 5 1 3 3 3 3",
    log:
        "logs/{sample}.log",
    wrapper:
        "master/bio/pytrf/findstr"
