rule mofa2:
    input:
        "{data}.parquet",
    output:
        "{data}.hdf5",
    log:
        "log/{data}.log",
    params:
        scale_groups="FALSE",  # set to TRUE if groups have different ranges/variances
        scale_views="FALSE",  # set to TRUE if views have different ranges/variances
    wrapper:
        # "master/bio/mofa2"
        "file://.."
