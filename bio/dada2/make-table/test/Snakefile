rule dada2_make_table:
    input:
    # Inferred composition (se) or merged composition (pe)
        expand("denoised/{sample}.1.RDS", sample=['a','b']) # se version
      #Â expand("merged/{sample}.RDS", sample=['a','b'])     # pe version
    params:
        names=['a','b'] # Sample names instead of paths
